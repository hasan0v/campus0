{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "builds": [
    {
      "src": "api/handler.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/handler.cjs"
    },
    {
      "src": "/static/(.*)",
      "dest": "/public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/handler.cjs"
    }
  ]
}
